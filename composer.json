{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "alkoumi/laravel-hijri-date": "^1.0",
        "babenkoivan/telegram-notifications": "^1.0",
        "barryvdh/laravel-cors": "0.11.0",
        "barryvdh/laravel-debugbar": "~2.4",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "hisorange/browser-detect": "^3.1",
        "igaster/laravel_cities": "^1.3",
        "intervention/image": "^2.5",
        "itsgoingd/clockwork": "^3.1",
        "laravel-notification-channels/telegram": "^0.0.5",
        "laravel/framework": "5.4.*",
        "laravel/passport": "^4.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4.0",
        "maatwebsite/excel": "^2.1",
        "mpociot/botman": "1.5.9",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.3",
        "setasign/fpdi-protection": "^2.0",
        "simplesoftwareio/simple-qrcode": "~1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
